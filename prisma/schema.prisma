generator client {
  provider        = "prisma-client-js"
  // https://www.prisma.io/docs/orm/reference/prisma-client-reference#omit-preview
  previewFeatures = ["omitApi"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Test {
  id         Int      @id @default(autoincrement())
  created_at DateTime @default(now()) @db.Timestamptz(6)
}

model Bookmark {
  id          Int      @id @default(autoincrement())
  created_at  DateTime @default(now()) @db.Timestamptz(6)
  title       String
  description String
  url         String
  image       String
  is_deleted  Boolean  @default(false)
}
