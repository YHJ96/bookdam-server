generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["omitApi"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Bookmarks {
  id          Int      @id(map: "Bookmark_pkey") @default(autoincrement())
  created_at  DateTime @default(now()) @db.Timestamptz(6)
  title       String
  description String
  url         String
  image       String
  is_deleted  Boolean  @default(false)
}

model Tags {
  id   BigInt @id(map: "tags_pkey") @default(autoincrement())
  name String @unique(map: "tags_name_key")
}
